{
  "extends": "tslint:recommended",
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "ordered-imports": [
      {
        "import-sources-order": "any",
        "named-imports-order": "any"
      }
    ],
    "no-trailing-whitespace": true,
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "object-literal-sort-keys": false,
    "interface-name": false
  }
}
